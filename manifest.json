{
  "manifest_version": 3,
  "name": "Hide Google AI Overviews",
  "version": "1.0",
  "description": "Hide annoying Google AI Overviews.",
  "icons": {
    "16": "icons/strikeAI16.png",
    "48": "icons/strikeAI48.png",
    "128": "icons/strikeAI128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.google.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ]
}
